* {
  bg: #272822;
  fg: #f8f8f2;
  accent: #f4bf75;
   
  background-color: @bg;
  text-color: @fg;
}

prompt, entry {
  text-color: #a6e22e;
  padding: 8px;
}

prompt {
    background-color: @accent;
    text-color: @bg;
    margin: 0 0 10px 0;
}

element {
  padding: 6px 4px;
}

element selected {
  background-color: @fg;
  text-color: @bg;
}
